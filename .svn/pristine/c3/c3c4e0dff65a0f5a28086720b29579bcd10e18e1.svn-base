<body>


    <section class="header">
        <nav class="navbar navbar-expand-lg navbar-light bg-light" id="header_navbar">

            <div class="navbar-brand-container p-l-20 ">
                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">
                    <img src="~/images/logo_TM.png" class="img-responsive MasterPage-Logo" />
                    <span>  Enterprise Ver.1.0 </span>
                </a>
            </div>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="">
            </div>

            <ul class="navbar-nav ml-auto" id="navbarSupportedContent">
                <li class="nav-item dropdown p-r-30">
                    <a class="nav-link dropdown-toggle top-3" href="#" id="navbarDropdown1" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <i class="fa fa-map-marker-alt"></i>  Location
                    </a>
                    <div class="dropdown-menu dropdown-menu-right animated upArrowForDropdown" aria-labelledby="navbarDropdown1" id="chkLoc">
                        <label class="dropdown-item"> Bengaluru <input type="radio" name="location" value="" /></label>
                        <label class="dropdown-item"> Chennai <input type="radio" name="location" value="" /></label>
                        <label class="dropdown-item"> Hyderabad <input type="radio" name="location" value="" /></label>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown2" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Hi  @User.Identity.Name &nbsp; <i class="far fa-user-circle c-white user-logo">  </i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right animated fadeIn" aria-labelledby="navbarDropdown2" id="divSignOutBox">
                        <div class="row">
                            <a class="text-center quickMenuItem" href="">
                                <span class="text-center"><i class="fa fa-info-circle"></i></span>
                                <p class="text-center"> About us </p>
                            </a>

                            <a class="text-center quickMenuItem" href="">
                                <span class="text-center"><i class="far fa-file-alt"></i></span>
                                <p class="text-center"> View Profile </p>
                            </a>
                            <a class="text-center quickMenuItem" href="">
                                <span class="text-center"><i class="fa fa-key"></i></span>
                                <p class="text-center">Reset Password </p>
                            </a>
                            <a class="text-center quickMenuItem" href="">
                                <span class="text-center"><i class="fa fa-cog"></i></span>
                                <p class="text-center"> Settings </p>
                            </a>
                            <a class="text-center quickMenuItem" href="">
                                <span class="text-center"><i class="far fa-comments"></i></span>
                                <p class="text-center"> Feedback </p>
                            </a>
                            <a class="text-center quickMenuItem" href="">
                                <span class="text-center"><i class="fa fa-cogs"></i></span>
                                <p class="text-center"> FAQ </p>
                            </a>
                        </div>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item bg-whitesmoke" asp-controller="Login" asp-action="Login"><i class="fas fa-sign-out-alt"></i> Sign out</a>
                    </div>
                </li>
            </ul>
        </nav>
    </section>


    <div class="header p-y-10 mb-10">
        @*<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>*@
        <p class="form-name d-inline" id="lblFormName"> </p>
        @*<p class="errorText"><i class="fas fa-times-circle"></i></p>*@
        <div class="message-notification d-inline p-r-15">
            <a href="#" class="f-18 m-r-30  material-icons mdl-badge mdl-badge--overlap" data-badge="5" id="message-notification"> <i class="far fa-envelope"></i> </a>
            <a href="#" class="f-18 m-r-30 material-icons mdl-badge mdl-badge--overlap" data-badge="7" id="activity-notification"> <i class="far fa-bell"></i> </a>
            <a href="#" class="f-18" id="expand-notification"> <i class="fas fa-expand"></i> </a>
            <a href="#" class="f-18" id="compress-notification"> <i class="fa fa-compress"></i> </a>
        </div>
    </div>


    <div class="container-fluid body-content">
        @RenderBody()
    </div>
    <footer>
        <p>&copy; @System.DateTime.Now.Year - esyaEnterprise</p>
    </footer>



    <script>
        function getBaseURL()
        {
            var url = window.location.href.split('/');

            var baseUrl = url[0] + '//' + url[2] + '@Url.Content("~")';
            return baseUrl;
        }

        function fnAlert(msg, type) {
            $(".error-text").removeClass("animated fadeOut");
            var navheight = $("nav").innerHeight();
            $('.errorText').css({ 'top': navheight })
            var typeOfMsg = type.trim();
            if (typeOfMsg == 's') {
                $('#errorMessage').text(msg);
                $('#erroricon').html("<i class='fa fa-check'></i>");
                var totalHeightofErrorBox = $(".error-text").innerHeight() + $('#errorMessage').height() + $('#erroricon').height();

                $('.error-text').css({
                    'background-color': '#28a745',
                    'margin-top': -(totalHeightofErrorBox)
                });
                $(".error-text").addClass("active animated fadeInDown");


                setTimeout(function () {
                    $(".error-text").removeClass("active animated fadeInDown");

                }, 4000);

            }
            else {
                $('#errorMessage').text(msg);
                $('#erroricon').html("<i class='fas fa-info-circle'></i>");

                $(".error-text").addClass("active animated fadeInDown");
                var totalHeightofErrorBox = $(".error-text").innerHeight() + $('#errorMessage').height() + $('#erroricon').height();

                $('.error-text').css({
                    'background-color': '#dc3545',
                    'margin-top': -(totalHeightofErrorBox)
                });


                setTimeout(function () {
                    $(".error-text").removeClass("animated fadeInDown").addClass("animated fadeOut")
                    $(".error-text").removeClass("active ");

                }, 4000);

            }
        }
    </script>

    <script src="~/js/utility/cf-input-validation.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>
