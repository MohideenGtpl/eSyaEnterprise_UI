                        <div class="row m-0">
                            <div class="col-lg-12">
                                <h1 class="ga-heading">Projects</h1>
                            </div>
                        </div>
                    </section>
                </div>
                @*projects tab starts*@

                @*reports starts here*@
                <div class="tab-pane" id="reports" role="tabpanel" aria-labelledby="reports-tab">
