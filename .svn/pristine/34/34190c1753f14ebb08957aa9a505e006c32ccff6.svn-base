    top: -1px;
    margin-bottom: 15px;
}
@-webkit-keyframes calling {
    0% {
        background-color: #ff0408;
    }

    49% {
        background-color: #f78787;
    }

    50% {
        background-color: #ff0408;
    }

    99% {
        background-color: #f78787;
    }

    100% {
        background-color: #ff0408;
    }
}

@-moz-keyframes calling {
    0% {
        background-color: #ff0408;
    }

    49% {
        background-color: #f78787;
    }

    50% {
        background-color: #ff0408;
    }

    99% {
        background-color: #f78787;
    }

    100% {
        background-color: #ff0408;
    }
}

@keyframes calling {
    0% {
        background-color: #ff0408;
    }

    49% {
        background-color: #f78787;
    }

    50% {
        background-color: #ff0408;
    }

    99% {
        background-color: #f78787;
    }

    100% {
        background-color: #ff0408;
    }
}

.calling {
    -webkit-animation: calling 1s infinite;
    -moz-animation: calling 1s infinite;
    animation: calling 1s infinite;
