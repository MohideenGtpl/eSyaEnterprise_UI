/*Selectpicker Styles*/
.selectpicker+.btn, .selectpicker+.btn-light:not(:disabled):not(.disabled).active, .selectpicker+.btn-light:not(:disabled):not(.disabled):active, .show > .btn-light.dropdown-toggle {
    padding: 4px;
    background: #fff;
    border-top-color: transparent;
    border-right-color: transparent;
    border-left-color: transparent;
    border-bottom: 1px solid rgba(0,0,0,.12);
    font-size:13px !important;
    border-radius:0px !important;
        box-shadow:none !important;
}

    .selectpicker + .btn:focus, .bootstrap-select.dropdown-toggle:focus {
        /*border-bottom-color: #0f436f !important;*/
        outline: 0 !important;
        transition-duration: .2s;
        box-shadow: none !important; transition-timing-function: cubic-bezier(.4,0,.2,1);
    }

