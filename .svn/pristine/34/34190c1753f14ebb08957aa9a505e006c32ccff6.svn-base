/* Hide/rearrange for smaller screens */
@media screen and (max-width: 767px) {

    .repeater-heading {
        font-size: 12px;
        text-transform: uppercase;
        font-weight: 600;
        margin-bottom: 0px;
        float: left;
        width: 100%;
        padding-left: 30px;
        color: #23a8e0;
        padding-top: 3px;
        padding-bottom: 3px;
    }

    .repeater-content {
        display: inline;
        float: left;
        width: 100%;
        padding-right: 15px;
    }
        .repeater-content button{
            margin-right:0px !important;
        }
        #nav-tab {
            display: inline-flex;
            overflow-x: auto;
            white-space: nowrap;
            flex-wrap: nowrap;
            width: 100%;
        }

    footer p {
        text-align: center !important
    }

    .header .form-name {
        padding: 0px 0px 0px 15px !important
    }

    .p-r-50 {
        padding-right: 15px !important
    }

    .p-l-50 {
        padding-left: 0px !important;
    }

    .container-fluid {
        padding-left: 15px !important;
        padding-right: 15px !important
    }

    .ui-state-default, .ui-widget-content .ui-state-default,
    .ui-jqgrid-view,
    .ui-widget.ui-widget-content {
        width: 100% !important;
    }

    .ui-jqgrid-btable, .ui-jqgrid-htable {
        width: auto !important;
        overflow: auto !important;
    }

    .ui-jqgrid-bdiv, .ui-jqgrid-hdiv, ui-jqgrid-pager {
        width: auto !important;
        overflow: auto !important;
    }
    /* Hide captions */
    .carousel-caption {
        display: none;
    }

    .dropdownMenuDiv {
        display: block;
    }
    .header nav {
        padding: .5rem 1rem;
        box-shadow: 0 0px 0px rgba(0, 0, 0, 0.16), 0 0px 3px rgba(0, 0, 0, 0.23);
    }
    body, .ui-jqgrid .ui-jqgrid-view {
        font-size: 13px !important;
    }
}
