<div class="row padtop15">
    <div class="col-lg-4 col-md-4 col-sm-6">
        <div class="form-group">
            <label> Customer Location</label>
            <input type="text" name="" value="" class="form-control" />
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6">
        <div class="default-checkbox">
            <label class="mdl-checkbox mdl-js-checkbox mdl-js-ripple-effect" for="chkValidateLimit">
                <input type="checkbox" id="chkValidateLimit" class="mdl-checkbox__input">
                <span class="mdl-checkbox__label">Is Location Default</span>
            </label>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6">
        <div class="form-group">
            <label>ISD Code</label>
            <select class="selectpicker" data-live-search="true" id="" title="" data-size="8">
                <option> Choose ISD Code</option>
            </select>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6">
        <div class="form-group">
            <label> Contact Person</label>
            <input type="text" name="" value="" class="form-control" />
        </div>
    </div>
    <div class="col-lg-8 col-md-8 col-sm-12">
        <div class="form-group">
            <label> Address</label>
            <input type="text" name="" value="" class="form-control" />
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6">
        <div class="form-group">
            <label> Registered Mobile Number</label>
            <input type="text" name="" value="" class="form-control" />
        </div>
    </div>
    <div class="col-lg-6 col-md-5 col-sm-6">
        <div class="form-group">
            <label> Email ID</label>
            <input type="text" name="" value="" class="form-control" />
        </div>
    </div>
    <div class="col-lg-2 col-md-3 col-sm-6">
        <div class="form-group">
            <label>Active Status</label>
            <select class="selectpicker" data-live-search="true" id="" title="" data-size="8">
                <option> Choose Status</option>
                <option>Active</option>
                <option>Inactive</option>
            </select>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-6">
        <div class="form-group">
            <label> Alternate Mobile Number</label>
            <input type="text" name="" value="" class="form-control" />
        </div>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 text-center">
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button m-r-15" id="btnSaveCustomerContact">
            Save
        </button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect  cancel-button" id="btnCancel" onclick="fnCloseForm()"> Cancel</button>

    </div>
</div>