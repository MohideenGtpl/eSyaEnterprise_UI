<div class="row padbot10 padtop15">
    <div class="col-lg-5 col-sm-6 col-md-5">
        <div class="form-group">
            <label> Item description</label>
            <input id="txtItemDescriptionforPartNumber" class="form-control" type="text" autocomplete="off" maxlength="75" />


        </div>
    </div>

    <div class="col-lg-2 col-sm-4 col-md-2">
        <div class="form-group">
            <label>Part Number</label>
            <input id="txtPartNumber" class="form-control" type="text" autocomplete="off" maxlength="15" />

        </div>
    </div>

    <div class="col-lg-3 col-sm-4 col-md-3">
        <div class="form-group">
            <label>Part Description</label>
            <input id="txtPartDescription" class="form-control" type="text" autocomplete="off" maxlength="100" />

        </div>
    </div>
    <div class="col-lg-2 col-sm-4 col-md-3">
        <div class="form-group">
            <label class="lable-color"> Status</label>
            <select class="selectpicker" id="cbostatusforPartNumber">
                <option value="true">Active</option>
                <option value="false">Inactive</option>
            </select>
        </div>
    </div>

</div>
<div class="row padbot10">
    <div class="col-lg-12 col-md-12 col-sm-12 text-center">
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button mr-10" id="btnPartNumber"> Save</button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect  cancel-button" onclick="fnCloseForm()"> Cancel</button>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 form-group">
        <table id="jqgCustomerPartNumber"></table>
        <div id="jqpCustomerPartNumber"></div>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 text-center">
        <hr />
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent btn-back mr-10 c-white" onclick="fnCloseCustomerDetails()"><i class="fas fa-chevron-left"></i> Back</button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button" id="btnSave" onclick="fnCloseForm()">
            <i class="fa fa-save"></i> Save
        </button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect  cancel-button" id="btnCancel"><i class="fa fa-redo"></i> Cancel</button>

    </div>
</div>