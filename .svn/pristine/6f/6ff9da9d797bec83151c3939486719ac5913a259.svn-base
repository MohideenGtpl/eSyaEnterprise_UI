<div class="modal fade in" id="PopupBedDetails" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog  modal-md  " style="box-shadow:none">
        <div class="modal-content">
            <!-- Modal Header -->
            <div class="modal-header popupHeader">
                <h4 class="modal-title">Bed Details</h4>
                <button type="button" class="close ml-auto" data-dismiss="modal"> <i class="fa fa-times"></i></button>
            </div>
            <div class="modal-body">

                <div class="col-lg-12  col-md-12 col-sm-12 pad0">

                    <div class="form-group">
                        <label class="lblBlock">Patient Name</label>
                        <label class="lblPatientDetails">Mr. Ahmed Mohammed Shakul Hamed</label>
                    </div>
                    <div class="form-group">
                        <label class="lblBlock">Patient ID:</label>
                        <label class="lblPatientDetails">AHM000002500</label>
                    </div>
                    <div class="d-inline-b mr-10 form-group">
                        <label class="lblInline">Age</label>
                        <label class="lblPatientDetails">45</label>
                    </div>

                    <div class="d-inline-b mr-10 form-group">
                        <label class="lblInline">Gender</label>
                        <label class="lblPatientDetails">Male</label>
                    </div>
                    <div class="d-inline-b mr-10 form-group">
                        <label class="lblInline">Date of Admission</label>
                        <label class="lblPatientDetails">02/02/2021</label>
                    </div>

                    <div class="form-group">
                        <label class="lblBlock">Doctor Name:</label>
                        <label class="lblPatientDetails">Dr.Kabali Khan</label>
                    </div>

                    <div class="form-group">
                        <label class="lblBlock">Bed Number</label>
                        <label class="lblPatientDetails" id="lblBedNumber">A1-03</label>
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="mdl-button mdl-js-button mdl-button--raised  mdl-button--accent primary-button mb-10"> Admit</button>
                <button type="button" class="mdl-button mdl-js-button mdl-button--raised  mdl-button--accent primary-button mb-10"> Discharge</button>
                <button type="button" class="mdl-button mdl-js-button mdl-button--raised  mdl-button--accent primary-button mb-10"> Transfer</button>

            </div>
        </div>
    </div>
</div>
 