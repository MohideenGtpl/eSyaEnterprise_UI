
@{
    ViewData["Title"] = "V_701_00";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/css/Masters/BedManagement.css" rel="stylesheet" />
@section scripts{
<script src="~/js/eSyaInPatient/BedManagement.js"></script>
