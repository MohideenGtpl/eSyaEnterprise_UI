<style>
    #wardSection {
        background: #fafafa;
        margin: 0px 0px 10px;
        padding: 5px 0px;
        border: 1px solid #eaeaea;
    }
    #bedSection .outerbox {
        width: 15.62%;
        padding: 0px 0rem;
        border-left: 4px solid #ddd;
        cursor: pointer;
        display: inline-flex;
        background: #fff;
        border-radius: 4px;
        margin: 0px 0.75rem 1.25rem 0px;
        box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 0px 1px;
    }
    #bedSection .innerbox p {
        font-size: 11.25px;
        display: block;
        color: #797172;
    }
    #bedSection .innerbox {
        padding: 5px 0px;
        width: 100%;
        margin:0px;
    }

    #bedSection .innerbox .fa-bed {
        font-size: x-large;
        color: #e3e3e3;
        height: 100%;
    }
    #bedSection h4 {
        margin: 0px -15px 10px;
        font-size: 14px;
        text-transform: capitalize;
        font-weight: 600;
        color: #505050;
        background: #fff;
        padding: 0px 0px 0px;
        letter-spacing: 1.15px;
    }
    .innerbox .patientName {
        height: 20px;
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
    }
</style>
<section id="wardSection">
    <div class="row mar0">
        <div class="col-lg-2 col-md-2 col-sm-6 pl-3">
            <select class="form-control">
                <option value="value">Select</option>
                <option>All</option>
                <option>General</option>
                <option>Semi Private</option>
                <option>Private</option>
                <option>Deluxe</option>
                <option>Semi Deluxe</option>
                <option>AC</option>
            </select>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-6 ">
            <select class="form-control">
                <option value="value">Select bed status</option>
                <option>All</option>
                <option>Vacant</option>
                <option>Admitted</option>
                <option>Maintenance</option>
                <option>Deluxe</option>
                <option>HouseKeeping</option>

            </select>
        </div>


        <div class="col-lg-6 col-md-6 col-sm-6">
            <ul class="legendItems">
                <li>Bed Status:</li>
                <li><span class="square vacant"></span> - Vacant</li>
                <li><span class="square admitted"></span> - Admitted</li>
                <li><span class="square maintenance"></span> - Maintenance</li>
                <li><span class="square housekeeping"></span> - HouseKeeping</li>
                <li><span class="square selected"></span> - Selected</li>
            </ul>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-6">
            <button type="button" class="btn btn-link">More options</button>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="row mar0">
                <div class="col-lg-12 col-md-12 col-sm-12" style="margin-top:10px;background:aliceblue;height:100%;">

                </div>
            </div>
        </div>
    </div>

</section>

