<div class="row">
    <div class="col-lg-12 form-group">
        <p class="subheading"> Personal Information</p>
        <hr class="left" />
    </div>
    <div class="col-lg-3 col-md-6 form-group">
        <label>Date of birth</label>
        <input type="text" class="form-control datepicker" />
    </div>
    <div class="col-lg-3 col-md-6 form-group">
        <label>Blood Group</label>
        <select class="form-control selectpicker">
            <option value="value">Select</option>
        </select>
    </div>
    <div class="col-lg-3 col-md-6 form-group">
        <label>Mother Tongue</label>
        <select class="form-control selectpicker">
            <option value="value">Select</option>
        </select>
    </div>
    <div class="col-lg-3 col-md-6 form-group">
        <label>Religion</label>
        <select class="form-control selectpicker">
            <option value="value">Select</option>
        </select>
    </div>
    <div class="col-lg-3 col-md-6 form-group">
        <label>Caste</label>
        <select class="form-control selectpicker">
            <option value="value">Select</option>
        </select>
    </div>
    <div class="col-lg-3 col-md-6 form-group">
        <label>Unique ID</label>
        <select class="form-control selectpicker">
            <option value="value">Select</option>
        </select>
    </div>
    <div class="col-lg-3 col-md-6 form-group">
        <label>Unique ID Info</label>
        <input type="text" class="form-control" />
    </div>
</div>

<div class="row form-group">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <table id="jqgLanguagesKnown"></table>
        <div id="jqpLanguagesKnown"></div>
    </div>
</div>

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 form-group">
        <p class="subheading"> Contact Information</p>
        <hr class="left" />
    </div>
    <div class="col-lg-6 col-md-6 col-sm-6 form-group">
        <label>Contact Person</label>
        <input type="text" class="form-control" />
    </div>
    <div class="col-lg-2 col-md-3 col-sm-3 form-group">
        <label>Contact Person Mobile No</label>
        <input type="text" class="form-control" />
    </div>
    <div class="col-lg-2 col-md-3 col-sm-3 form-group">
        <label>Landline No</label>
        <input type="text" class="form-control" />
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 form-group">
        <label>Permanent Address</label>
        <input type="text" class="form-control" />
    </div>
    <div class="col-lg-2 col-md-3 col-sm-4 col-xs-12 form-group">
        <div class="default-checkbox flex">
            <label class="mdl-checkbox mdl-js-checkbox mdl-js-ripple-effect" for="chkSameAsPermanent">
                <input type="checkbox" id="chkSameAsPermanent" class="mdl-checkbox__input">
                <span class="mdl-checkbox__label"> Same as Permanent</span>
            </label>
        </div>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 form-group">
        <label>Current Address</label>
        <input type="text" class="form-control" />
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 text-center">
        <hr />
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent btn-back mr-10 c-white"><i class="fas fa-chevron-left"></i> Back</button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button mr-10" id="btnSaveEmployeeDetails" onclick="fnSaveCustomerDetails()">
            <i class="fa fa-save"></i>   Save
        </button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect  cancel-button" id="btnCancel"><i class="fa fa-redo"></i> Cancel</button>

    </div>

</div>

