
@{
    ViewData["Title"] = "V_2002_00";
    Layout = "~/Views/Shared/_Layout.cshtml";
} 
<style>
    .divFixedBar {
        position: fixed;
        top: 107px;
        height: 36px;
        border-bottom: 1px solid #ccc;
        width: 100%;
        margin: 0px -15px;
    }

        .divFixedBar p:not(span) {
            font-size: 15px;
            margin: 0px 5px;
            display: inline-block;
            padding: 8px 15px 0px;
        }

    [id^="lbl"] {
        color: gold;
        font-weight: 600;
        letter-spacing: 0.75px;
        font-size: 16px;
    }

    .buttonSection {
        margin-right: 15px !important;
        position: relative;
        display: inline-block;
        position: absolute;
        right: 0px;
        cursor: pointer;
    }

</style>
<link href="~/css/emergency.css" rel="stylesheet" />
@section scripts{
<script src="~/js/eSyaOutPatient/emergency.js"></script>
}

<div class="divFixedBar">
    <p><span> ER No:</span> <span id="lblERNO">XXXXXXXX10</span></p>
    <p><span> Date:</span> <span id="lblDate">16/02/2021</span></p>
    <p><span> Patient Name:</span> <span id="lblPatientName">Arun Kumar</span></p>

    <div class="buttonSection">
        <button class="mdl-button mdl-js-button mdl-button--raised primary-button mr-10"> <i class="far fa-save f-16"></i>  <span class="lblTextForButton">Save</span></button>
