body {
}
div[class*='col']{
    padding-right:0.5rem !important;
    padding-left:0.5rem !important;
}
.default_checkbox_inline {
    display: inline-flex;
    align-items: end;
}
.default_checkbox_inline label{display:inline; padding-right:10px; width:100px;}

.wrapByBox {
    background: #fff;
    border-radius: 5px;
    box-shadow: 0px 0px 2px rgba(0,0,0,0.2);
    margin: 4px 0px 0px 0px;
    margin-bottom: 0px;
    padding: 5px 10px;
    box-shadow: rgba(0, 0, 0, 0.18) 0px 2px 4px;
    height:100%;
}
.wrapByRow {
    background: #fff;
    border-radius: 5px;
    box-shadow: 0px 0px 2px rgba(0,0,0,0.2);
    margin: 4px 7px 0px 7px;
    margin-bottom: 0px;
    padding: 5px 10px;
    box-shadow: rgba(0, 0, 0, 0.18) 0px 2px 4px;
    width:100%;
}
hr.left {
    border-radius: 0px;
    height: 2px;
    background-color: #fbd427;
    width: 75px;
    margin: 2px 0px !important;
}
.divFixedBar .labelname {
    font-size: 11px;
    font-weight: 400;
    color: #a49f9f;
 }
.table-borderless input[type="checkbox"], .table-borderless  div{
    display: inline;
}
.table-borderless tbody + tbody, .table-borderless td, .table-borderless th, .table-borderless thead th{
    padding:2.5px 5px;
}
.pageSection{
    margin-bottom:15px;
}

#mainContent {
    scroll-behavior: smooth;
    background: #fff;
    position: relative;
    right: 0px;
    overflow-y: auto;
    overflow-x: hidden;
    z-index: 1;
    padding: 0px 15px 0px 15px;
    display: block;
    margin-right: 0px;
    width: 100%;
  
}
#floatButtons{
    padding-right:0px !important;
  }
#floatButtons .btntext {
    font-size: 11px !important;
    position: relative;
    top: -4px;
    color: #fdd42a;
}
#floatButtons button {
    display: inline;
    height: 45px !important;
    text-align: center;
    font-size: 12px !important;
    border-right: 1px solid #e8e8e8;
    margin: 0px 0px 0px 0px;
    width: 45px;
    padding: 0px 0px;
    border-radius: 0px;
    white-space: nowrap;
    background: #555;
}
    #floatButtons button:nth-of-type(1){
        border-left:1px solid #e8e8e8;
    }
    #floatButtons button svg {
        display: block;
        font-size: 22px;
        margin: auto;
        color: #fada28 !important;
        position: relative;
        top: 4px;
        width: 100%;
        color: #fdd42a;
    }
.divFixedBar {
position: fixed;
top: 107px;
height: auto;
border-bottom: 1px solid #ccc;
width: 100%;
margin: 0px -15px;
}
.divFixedBar p:not(span) {
    margin: 0px 0px;
    display: block;
    padding: 0px 0px 0px;
}
.pageSection h4 {
    color: #5d5d5d;
    font-size: 16px;
    font-weight: 600;
    letter-spacing: 1px;
    margin: 0px -15px;
    padding: 5px 15px;
    background: #fffcec;
    box-shadow: rgba(50, 50, 93, 0.25) 0px 2px 5px -1px, rgba(0, 0, 0, 0.3) 0px 1px 3px -1px;
}
#lblpatientname{
    font-size:14px;
}
.infoTable th, .infoTable td{
    font-style:italic;
    padding:.25rem !important;
    font-size:12px !important;
}
.scaleLines {
    width: max(9.75%,5.26vw);
    height: 30px;
    border: 1px solid black;
    border-bottom: none;
    display: inline-block;
    border-width: 5px;
    border-right:none;
}
    .scaleLines:last-child {
        border-right: 1px solid black;
        border-width: 5px;
    }
    .scaleLines:nth-of-type(1) {
        border-color: #308945;
    }
    .scaleLines:nth-of-type(2) {
        border-color: #72aa35;
        margin-left: -5px;
    }
    .scaleLines:nth-of-type(3) {
        border-color: #a8ca2c;
        margin-left: -5px;
    }
    .scaleLines:nth-of-type(4) {
        border-color: #f2f21d;
        margin-left: -5px;
    }

    .scaleLines:nth-of-type(5) {
        border-color: #f3d112;
        margin-left: -5px;
    }

    .scaleLines:nth-of-type(6) {
        border-color: #f38e05;
        margin-left: -5px;
    }
    .scaleLines:nth-of-type(7) {
        border-color: #ef6401;
        margin-left: -5px;
    }

    .scaleLines:nth-of-type(8) {
        border-color: #ea5201;
        margin-left: -5px;
    }

    .scaleLines:nth-of-type(9) {
        border-color: #df1701;
        margin-left: -5px;
    }

    .scaleLines:nth-of-type(10) {
        border-color: #d70401;
        margin-left: -5px;
    }
.lblNumbers {
    width: max(8.6%, 4.5vw);
    text-align: left;
    display: inline-block;
    font-weight: 600;
    padding-bottom: 5px;
}
#divScaleLines, #divScaleNumbers {
    padding-left: 65px;
}
    .lblNumbers:last-child{
        width:auto !important;
    }
    .c-lightgrey {
        color: #919191 !important;
    }
.ratings > label {
    margin-right: 30px;
    cursor: pointer;
    margin: auto;
    text-align: center;
    width: max(16.6%,16vw);
}
    .ratings > label > span {
        margin: auto;
        text-align: center;
        font-weight: 300;
        color: #444343;
        letter-spacing: 0.5px;
    }
    .ratings > label > img {
        width: 40px;
        height: 40px;
        filter: grayscale(1);
    }
[class^="firework"] {
    position: absolute;
    width: 0.1rem;
    height: 0.1rem;
    border-radius: 50%;
}
.ratings label.active img, .ratings label:hover img {
    filter: grayscale(0);
    transition: 0.5s;
    transform: scale(1.2);
    /*-webkit-animation: firework-sm 1.2s both infinite;
    animation: firework-sm 1.2s both infinite;
    -webkit-animation-delay: 1.5s;
    animation-delay: 1.5s;
    top: 15%;
    left: 75%;*/
}
@keyframes firework-sm {
    0%, 100% {
        opacity: 0;
    }

    10%, 70% {
        opacity: 1;
    }

    100% {
        box-shadow: -0.5rem 0rem 0 #fff, 0.5rem 0rem 0 #fff, 0rem -0.5rem 0 #fff, 0rem 0.5rem 0 #fff, 0.35rem -0.35rem 0 #fff, 0.35rem 0.35rem 0 #fff, -0.35rem -0.35rem 0 #fff, -0.35rem 0.35rem 0 #fff;
    }
}
