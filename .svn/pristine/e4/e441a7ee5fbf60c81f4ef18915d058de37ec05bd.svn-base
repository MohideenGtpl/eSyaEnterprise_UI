#txtOTP {
    padding-left: 17px;
    letter-spacing: 38px;
    border: 0;
    background-image: linear-gradient(to left, #7a7273 70%, transparent 0%);
    background-position: bottom;
    background-size: 52px 2px;
    background-repeat: repeat-x;
    background-position-x: 44px;
    width: 338px;
    min-width: 292px;
    font-size: 26px;
    font-weight: 600;
    background-color: transparent;
    color: #797172;
}
    #txtOTP:focus, #divInner:focus, #divOuter:focus{
        outline: 0px !important;
        border-color: transparent !important;
    }

    #divInner {
        left: 0;
        position: sticky;
    }

#divOuter {
    width: 318px;
    overflow: hidden;
}
.OTPStatement {
    margin-bottom: 0px !important;
    padding: 10px 6px !important;
    width: 100% !important;
    background: #f9f9f9;
