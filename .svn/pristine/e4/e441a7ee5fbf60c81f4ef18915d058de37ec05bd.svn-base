.form-control {border: none !important; border-bottom: 1px solid #797172 !important;border-radius: 0px !important;line-height: 20px;
height: 30px !important; padding:0px !important;}
.form-control:focus {outline: none !important;box-shadow: none !important;border-bottom: 1px solid gold !important;}
    @media screen and (min-width:1301px) {
        .loginContainer { width: 764px}
        .mainLogo {position: absolute;bottom: -30px;z-index: 21 !important;left: 40%;width:174px;/*height:57px;*/}
        .smallDeviceLogo {display: none;}
        .login-form { margin-left: -15px;}
    }

    @media screen and (max-width:1300px) and (min-width:1025px) {
        .loginContainer {  width: 764px}
         .mainLogo { position: absolute;bottom: -30px;z-index: 21 !important;left: 40%;width: 174px;/*height: 57px;*/}
         .smallDeviceLogo {display: none;}
         .loginForm {margin-left: -15px;}
    }

    @media screen and (max-width:1024px) and (min-width:992px) {
        .login-form h4 {margin: 0px !important;text-align: center;padding: 30px 0px 20px 0px !important;}
        .loginContainer {width: 760px}
        .mainLogo {position: absolute; bottom: -30px;z-index: 21 !important;left: 40%;width: 174px;/*height: 57px;*/}
        .smallDeviceLogo { display: none;}
        .loginForm {border-left: 1px solid #dad9d9;margin-left: -15px;}
    }

    @media screen and (max-width:991px) and (min-width:768px) {
        .loginContainer { width: 660px}
        .mainLogo {position: absolute;bottom: -30px;z-index: 21 !important;left: 30%;width: 174px;/*height: 57px;*/}
        .smallDeviceLogo {display: none;}
        .login-form {border-left: 1px solid #dad9d9;margin-left: -15px;}
        .login-form h4 {margin: 0px !important;text-align: center;padding: 30px 0px 20px 0px !important;}
    }


    @media screen and (max-width:767px) and (min-width:501px) {
        .login-form h4 { margin: 0px !important; text-align: center; padding: 0px 0px 20px 0px !important;}
        #errormsg {position: absolute;bottom: 12px;left: 0px;}
        .loginContainer { width: 420px;}
        .leftImage { display: none}
        .login-form {border-left: 1px solid #dad9d9; margin-left: -15px;}
        .smallDeviceLogo {display: inline-block;vertical-align: middle; width: 126px;}
        .mainLogo { display: none;}
        .smallDeviceLogo {display: inline-block; position: relative; top: -20px;}
    }

    @media screen and (max-width:500px) and (min-width:300px) {
        .login-form {
          border-right:  1px solid #dad9d9 !important
        }
        .login-form h4 {
            margin: 0px !important;
            text-align: center;
            padding: 0px 0px 20px 0px !important;
        }
        #errormsg {position: absolute;top: -10px; left: 0px;}
        .loginContainer { width: 85%;  margin-top: 20%;}
        #selectLanguage select{
            margin-top:15px;
        }
        .leftImage {
            display: none;
        }
        .login-form { border-left: 1px solid #dad9d9;}
        .socialIconsaligment { text-align: center;}
        .mainLogo {display: none;opacity: 0; }
        .smallDeviceLogo {width: 120px;margin: auto;position: relative;top: -20px;}
        .languageDiv {
            position: absolute;
            top: 4px;
            left: 22px;
            font-size: 11px;
            border-top: 0px !important;
            z-index: 2;
            width:auto;
        }
        .pdngContent {padding: 15px}
        .imgSocialIcons:hover {opacity: 0.0;}
    }
