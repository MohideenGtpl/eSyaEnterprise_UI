
@{
    ViewData["Title"] = "V_1014_00";
}
@section scripts{ 
<script src="~/js/eSyaMaterial/ItemReorderLevel.js"></script>
}
<section>
    <div class="row">
        <div class="col-lg-3 col-md-3 col-sm-12">
            <div class="form-group">
                <label class="lable-color">Business Key</label>
                <select class="selectpicker" data-live-search="true" id="cboBusinessKey" title="Select" data-size="8"></select>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-12">
            <div class="form-group">
                <label class="lable-color">Store</label>
                <select class="selectpicker" data-live-search="true" id="cboStore" title="Select" data-size="8"></select>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 form-group">
            <table id="jqgItemReorderLevel"></table>
            <div id="jqpItemReorderLevel"></div>
        </div>
        <div class="col-lg-12 text-center">
            <hr />
            <button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button mr-10" id="btnSaveParameters" onclick="fnSaveParameters();">
                Save
            </button>
            <button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect  cancel-button cancel-button" data-dismiss="modal" id="#" onclick="fnClearFields()"> Cancel</button>
        </div>
    </div>
</section>

