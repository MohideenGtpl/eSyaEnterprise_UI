    else if (type === "MED")
        editMedValuesCharts(e, actiontype);
    else if (type === "MDA")
        editMDAValuesCharts(e, actiontype);
    else if (type === "BM")
        editBMValuesCharts(e, actiontype);
    else if (type === "CO")
        editCOValuesCharts(e, actiontype);
}

