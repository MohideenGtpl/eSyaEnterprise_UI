            Utils_ClinicalCharts.setClinicalTemplate(data);
        });

    });
}
function editCOValuesCharts(e, actiontype) {
    var rowid = $(e.target).parents("tr.jqgrow").attr('id');
    var rowData = $('#jqgCO-GRID').jqGrid('getRowData', rowid);

    $("#PopupComplications").modal('show');
    _isUpdate = true;
    _intTransactionId = rowData.TransactionID;

    getClinicalInformationByID(rowData.TransactionID, function (data) {
        $.each(data, function (key, val) {
            setDate($('#dpComplicationsDate'), val.TransactionDate);
            $('#tpComplicationsTime').val(new Date(val.TransactionDate).timeToInput());
        });

        $.when(Utils_ClinicalCharts.populateClinicalTemplate("CO")).then(function () {
            Utils_ClinicalCharts.setClinicalTemplate(data);
        });

    });
}

function getClinicalInformationByID(transactionID, callback) {
    $.get(getBaseURL() + '/PatientClinicalForms/GetClinicalInformationValueByTransaction',
        {
            UHID: $('#hdUHID').val(),
            vNumber: $('#hdIPNumber').val(),
            transactionID: transactionID
        }, function (data, status) {
            callback(data);
        });
}
function getClinicalInformation(cltype,callback) {
    $.get(getBaseURL() + '/PatientClinicalForms/GetPreOperativeInformation',
        {
            UHID: $('#hdUHID').val(),
            vNumber: $('#hdIPNumber').val(),
            clType: cltype
        }, function (data, status) {
            callback(data);
        });
}

