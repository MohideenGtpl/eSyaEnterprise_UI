ul#menu-v, #menu-v ul {
    width: 278px;
    border: 1px solid rgba(190,190,190,0.3);
    list-style: none;
    margin: 0;
    padding: 0;
    z-index: 9;
    background:#fff;
}

#menu-v li {
    margin: 0;
    padding: 0;
    position: relative;
    background-color: #fff; /*fallback color*/
    /*background-color: rgba(0,0,0,0.5);*/
    transition: background 0.5s;
}
#menu-v li:hover {background-color: rgb(224, 242, 255);}
#menu-v a {
    font: normal 14px;
    border-top: 1px solid rgba(190,190,190,0.3);
    display: block;
    color: #212121;
    text-decoration: none;
    line-height: 44px;
    padding-left: 22px;
    position: relative;
}

#menu-v li:first-child a {border-top: 0;}
#menu-v a.arrow::after {
    content: '';
    position: absolute;
    display: inline;
    top: 50%;
    margin-top: -4px;
    right: 8px;
    border-width: 4px;
    border-style: solid;
    border-color: transparent transparent transparent #1094f8;
    transition: border-color 0.5s;
}

#menu-v li a.arrow:hover::after { border-color: transparent transparent transparent #CCCCCC;}
/*Sub level menu items
---------------------------------------*/
#menu-v li ul {
    min-width: 180px; /* Sub level menu min width */
    position: absolute;
    display: none;
    left: 100%;
    top: 50%;
    transform: translateY(-50%);
}

#menu-v li:hover > ul {display: block;}
