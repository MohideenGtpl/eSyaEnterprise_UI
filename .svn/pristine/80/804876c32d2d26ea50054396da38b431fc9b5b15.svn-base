body {
    background: #fff;
    min-height: unset !important;
    font-family: 'helvetica' !important;
}
@font-face {
    font-family: 'helvetica';
    src: url('../fonts/Helvetica.ttf') format('truetype');
}

.pad0 {
    padding: 0px !important;
}

.padleft0 {
    padding-left: 0px !important;
}

.padright0 {
    padding-right: 0px !important;
}

.pad1 {
    padding: 1px !important
}

.pad5 {
    padding: 5px !important
}

.pad15 {
    padding: 15px !important;
}

.mr-0 {
    margin-right: 0px !important
}
.padtop20{padding-top:20px !important;}
.mt-20{margin-top:20px !important;}
.p-y-20 {
    padding-top: 20px;
    padding-bottom: 20px;
}

.d-inline-b {
    display: inline-block
}

.fullwidth {
    width: 100% !important;
    max-width: 100% !important;
}

.loginContainer {
    margin: 5% auto 0px;
    padding: 0px;
    background: #FFF;
}

.mdl-button {
    text-transform: capitalize !important;
    line-height: 34px !important;
    height: 34px !important;
    font-size:16px !important;
}

h3 {
    font-size: 28px !important;
    font-weight: 600;
    text-transform: uppercase;
    text-align: center;
    margin: 0px;
}
label {
    font-size: 12px !important;
    top: 4px;
    display: block;
    margin-bottom: 0px;
    font-weight: 200 !important;
    color: #797172;
}
.form-control{
    background:transparent !important;
}
.mdl-textfield__label {
    margin-bottom: 0px !important;
}
.selectpicker + .btn,
.selectpicker +
.btn-light:not(:disabled):not(.disabled).active,
.selectpicker + .btn-light:not(:disabled):not(.disabled):active,
.show > .btn-light.dropdown-toggle {
    padding: 0px !important;
}
#btnBack {
    position: absolute;
    top: 45px;
    color: #08b3f7;
    cursor:pointer;
}

.form-group .bootstrap-select, .form-horizontal .bootstrap-select, .form-inline .bootstrap-select {
    padding: 7px !important;
    padding-left: 0px !important;
    width: 100% !important;
}
.rememPassword.mdl-checkbox__label {
    font-weight: 300 !important;
    font-size: 14px !important;
}

label span.mdl-checkbox__label {
    font-weight: 300 !important;
    font-size: 14px !important;
}

.shadowLite {
    box-shadow: 0 3px 2px rgba(0, 0, 0, 0.19), 0 3px 3px rgba(0, 0, 0, 0.23);
}

.shadowMedium {
    box-shadow: 0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23);
}

.shadowLarge {
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 6px rgba(0, 0, 0, 0.23);
}

.shadowMega {
    box-shadow: 0 14px 28px rgba(0, 0, 0, 0.25), 0 10px 10px rgba(0, 0, 0, 0.22);
}



.login-form {
    background: #fff;
    display: inline-block !important;
    height: 433px;
    border-radius: 5px;
    border: 1px solid #dad9d9;
    background-image: url('../images/e-logo.png');
    border-left: none;
    border-bottom-left-radius: 0px;
    border-top-left-radius: 0px;
    background-repeat: no-repeat;
    background-position: right;
}
 .login-form h4{
     margin:0px !important;
     text-align:center;
     padding:30px 0px 20px 0px !important;
 }


.loginHeading {
    margin: 24px 0px;
}

    .loginHeading img {
        margin: 0px 5px;
        width: 40px;
        height: 40px;
    }

.right-tilt {
    transform: skewX(-20deg)
}

.right-tilt-alt {
    transform: skewX(20deg)
}

.loginForm h3 {
    text-transform: capitalize;
}

.loginForm h3, .loginForm p {
    color: #000;
}

.loginForm label {
    font-weight: 300 !important
}

.loginButton {
    background:#797172 !important;
    color:gold!important;
}

.forgotPassword {
    color: #1590c0 !important;
    text-decoration: underline;
}

.mdl-textfield--floating-label.is-focused .mdl-textfield__label {
    color: #0f436f !important;
}

.mdl-textfield__label:after {
    background-color: #0f436f !important;
}

p {
    text-align: center;
    width: 70%;
    margin: auto;
    padding-bottom: 20px;
}

    p.footernote {
        font-style: italic;
        color: #000;
        text-align: left;
        padding: inherit;
        width: inherit;
    }

.mdl-menu__container.is-visible, .mdl-menu__container, .mdl-menu__container.is-visible .mdl-menu__outline {
    width: 100% !important
}

.mdl-menu {
    min-width: 100%
}


.fb_border {
    border: 1px solid #ccc;
    padding: 2px;
    border-radius: 25px;
    text-align: center;
    margin-left: 60px;
    margin-right: 60px;
    margin-top: 5px;
}

.padtop30 {
    padding-top: 30px
}

.pdngContent {
    padding: 33px 18px 33px 18px;
}

.socialIconsaligment {
    text-align: center;
}

.imgSocialIcons:hover {
    opacity: 0.4;
}

.getUserId {
   display:block;
    font-size: 10px;
    text-transform: uppercase;
    color: #989797;
    text-decoration: underline;
    position: relative;
    top: -12px;
    text-align:right;
}

    .getUserId:hover {
        color: black;
        text-decoration: underline;
    }
.mdl-textfield {
    width:100% !important;
    max-width: 100% !important;
}

.languageDiv {
    position: absolute;
    top: 4px;
    right: 23px;
    font-size: 12px;
    border-top: 0px !important;
    z-index: 2;
}
.f-11 {
    font-size: 11px !important;
}
    .languageDiv select {
        border: none !important;
       
    }
        .languageDiv select:focus {
            border: none !important;
            outline: none !important;
        }
 body.arabic input{
    text-align:right;
}
body.arabic label{
    float:right;
}
body.arabic h4{
    text-align:right;
}

body.arabic .getUserId {
    text-align: left !important;
}
.c-red{
    color:red;
}
.form-control {
    border: none !important;
    border-bottom: 1px solid #797172 !important;
    border-radius: 0px !important;
    line-height: 20px;
    height: 30px !important;
    padding:0px !important;
}
    .form-control:focus {
        outline: none !important;
        box-shadow: none !important;
        border-bottom: 1px solid gold !important;
    }

    @media screen and (min-width:1301px) {
        .loginContainer {
            width: 764px
        }

        .mainLogo {
            position: absolute;
            bottom: -30px;
            z-index: 21 !important;
            left: 40%;
        }

        .smallDeviceLogo {
            display: none;
        }

        .login-form {
            margin-left: -15px;
        }
    }

    @media screen and (max-width:1300px) and (min-width:1025px) {
        .loginContainer {
            width: 764px
        }

        .mainLogo {
            position: absolute;
            bottom: -30px;
            z-index: 21 !important;
            left: 40%;
        }

        .smallDeviceLogo {
            display: none;
        }

        .loginForm {
            margin-left: -15px;
        }
    }

    @media screen and (max-width:1024px) and (min-width:992px) {
        .login-form h4 {
            margin: 0px !important;
            text-align: center;
            padding: 30px 0px 20px 0px !important;
        }
        .loginContainer {
            width: 760px
        }

        .mainLogo {
            position: absolute;
            bottom: -30px;
            z-index: 21 !important;
            left: 40%;
        }

        .smallDeviceLogo {
            display: none;
        }

        .loginForm {
            border-left: 1px solid #dad9d9;
            margin-left: -15px;
        }
    }

    @media screen and (max-width:991px) and (min-width:768px) {
        .loginContainer {
            width: 660px
        }

        .mainLogo {
            position: absolute;
            bottom: -30px;
            z-index: 21 !important;
            left: 30%;
        }

        .smallDeviceLogo {
            display: none;
        }

        .login-form {
            border-left: 1px solid #dad9d9;
            margin-left: -15px;
        }
            .login-form h4 {
                margin: 0px !important;
                text-align: center;
                padding: 30px 0px 20px 0px !important;
            }
    }


    @media screen and (max-width:767px) and (min-width:501px) {
        .login-form h4 {
            margin: 0px !important;
            text-align: center;
            padding: 0px 0px 20px 0px !important;
        }
        #errormsg {
            position: absolute;
            bottom: 12px;
            left: 0px;
        }

        .loginContainer {
            width: 420px;
        }

        .leftImage {
            display: none
        }

        .login-form {
            border-left: 1px solid #dad9d9;
            margin-left: -15px;
        }

        .smallDeviceLogo {
            display: inline-block;
            vertical-align: middle;
            width: 126px;
        }

        .mainLogo {
            display: none;
        }

        .smallDeviceLogo {
            display: inline-block;
            position: relative;
            top: -20px;
        }
    }

    @media screen and (max-width:500px) and (min-width:300px) {
        .login-form h4 {
            margin: 0px !important;
            text-align: center;
            padding: 0px 0px 20px 0px !important;
        }
        #errormsg {
            position: absolute;
            top: -10px;
            left: 0px;
        }

        .loginContainer {
            width: 85%;
            margin-top: 10%;
        }

        .leftImage {
            display: none
        }

        .login-form {
            border-left: 1px solid #dad9d9;
        }

        .socialIconsaligment {
            text-align: center;
        }

        .mainLogo {
            display: none;
            opacity: 0;
        }

        .smallDeviceLogo {
            width: 120px;
            margin: auto;
            position: relative;
            top: -20px;
        }
        .languageDiv {
            position: absolute;
            top: 4px;
            right: 3px;
            font-size: 11px;
            border-top: 0px !important;
            z-index: 2;
        }
        .pdngContent {
            padding: 15px
        }

        .imgSocialIcons:hover {
            opacity: 0.0;
        }
    }

    .cancel-button {
    color: #fff !important;
    background: #e9555d !important;
}

.mar-savebtn {
    margin-right: 5px;
}

.sub-text-heading {
    text-align: center;
    font-size: 12px;
}

.rad-btn {
    font-size: 12px !important;
    font-weight: 600 !important;
}

.mdl-radio__inner-circle {
    background: #07b3f7 !important;
}

.login-pagebtn {
