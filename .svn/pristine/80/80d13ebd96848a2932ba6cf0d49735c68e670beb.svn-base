<div class="row">
    <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
        <div class="form-group">
            <label>Location</label>
            <select class="selectpicker" id="cboDoctorLocationChange"></select>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
        <div class="form-group">
            <label>Specialty</label>
            <select class="selectpicker" id="cboDoctorSpecialtyChange"></select>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
        <div class="form-group">
            <label>Clinic</label>
            <select class="selectpicker" id="cboDoctorClinicChange"></select>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
        <div class="form-group">
            <label>Period</label>
            <select class="selectpicker" id="cboDoctorPeriodChange"></select>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
        <div class="form-group">
            <label>Schedule Date</label>
            <input id="dtSheduleDate" class="form-control datepicker" type="text" autocomplete="off" onkeydown="return false"/>
        </div>
    </div>
    <div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
        <div class="form-group">
            <label>From Time</label>
            <input id="txtFRMDocSpecialtyChange"  class="form-control" type="time" autocomplete="off" data-time-format="H:i:s" />
        </div>
    </div>
    <div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
        <div class="form-group">
            <label>To Time</label>
            <input id="txtToDocSpecialtyChange" class="form-control" type="time" autocomplete="off" data-time-format="H:i:s" />
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
        <div class="default-checkbox">
            <label class="mdl-checkbox mdl-js-checkbox mdl-js-ripple-effect" for="chkActive">
                <input type="checkbox" id="chkActive" class="mdl-checkbox__input">
                <span class="mdl-checkbox__label">Active</span>
            </label>
        </div>
    </div>
</div>

<div class="row form-group">
    <div class="col-lg-12 col-md-12 col-sm-12 text-center">
        <hr />
         
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button mr-10" id="btnSaveDoctorSchedulerChange"><i class="far fa-save"></i>  Save</button>
        <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect  cancel-button" onclick="fnClearDoctorSchedulerChange()"><i class="fa fa-redo"></i> Clear</button>
   </div>
</div>

    <!--End Text Filed Doctor Scheduler Change -->
    <!-- Doctor Scheduler Change Grid-->
    <div class="row form-group">
        <div class="col-lg-12">
            <table id="jqgDocSchedulerChange"></table>
            <div id="jqpDocSchedulerChange"></div>
        </div>
    </div>
     