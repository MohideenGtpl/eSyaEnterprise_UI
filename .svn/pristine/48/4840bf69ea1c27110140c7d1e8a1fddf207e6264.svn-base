/*Custom Table for Parameter*/
#tbParameter{
    width:100% !important;
}
    #tbParameter th {
        padding: 5px;
        text-align: left;
        background-color: #4CAF50;
        background: #797172 !important;
        color: gold !important;
        border-right: 1px solid #8c8c8c !important;
        height: 34px;
        letter-spacing:1.5px;
    }
    #tbParameter td input[type=checkbox] {
        vertical-align: middle;
    }
    #tbParameter td, #customers th {
        
        border: 1px solid #ddd;
        padding: 2px 5px;
        height: 32px;
    }
        #tbParameter td{
            font-size:12px !important;
            
        }
        #tbParameter tr:nth-child(even) {
            background-color: #fffcec;
        }

#tbParameter tr:hover {
    background-color: #fbe2ff;
}
    #tbParameter th:first-child, #tbParameter td:first-child{
        text-align: center;
    }
    #tbParameter tr td .form-control {
        border: none !important;
        border-bottom: 1px solid #ced4da !important;
        border-radius: 0px !important;
        line-height: 20px;
        height: 21px !important;
        padding: 4px 0px !important;
        font-size: 12px !important;
        width: 70px;
        margin: auto;
    }

