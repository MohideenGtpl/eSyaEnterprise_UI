.dot {
    height: 21px;
    width: 21px;
    border-radius: 0px;
    display: inline-block;
    text-align: center;
    border: 1px solid #dedede;
}

    .dot:hover {
        color: #ffd700;
        background-color: #797172;
        cursor: pointer;
    }
    .dot.active {
        color: #ffd700;
        background-color: #797172;
        cursor: pointer;
    }

