body {
}
table.dataTable > thead .sorting_asc::before, table.dataTable > thead .sorting_desc::after{
    opacity:0;
}

table.dataTable > thead .sorting::before, table.dataTable > thead .sorting_asc::before, table.dataTable > thead .sorting_desc::before, table.dataTable > thead .sorting_asc_disabled::before, table.dataTable > thead .sorting_desc_disabled::befor {
    opacity: 0 !important;
}
.dashboard-bar {
    width: 100%;
    background: #fff6f6;
    margin: 0px -15px;
    padding: 10px 0px 10px 15px;
}
.dataTables_info{
    display:none;
}
.table{
    table-layout:fixed;
}
td{
   vertical-align:middle;
}
td svg{height:30px;}
tr{
    margin-bottom:10px;
}
    tr td[rowspan="3"] {
        vertical-align: middle;
        text-align: center;
        font-size: 20px;
        
        color: #030200;
        font-weight: bold;
        background: #f7f7f7;
    }
td[colspan="2"] {
    background: #555;
    color: gold;
}
tr td:first-child{
    width:20%;
}
/*tr td:not(:first-child){
    width:11.24%;
}*/
thead tr:nth-child(1) td, thead tr:nth-child(2) td {
    background: #fffcea;
    position: sticky;
    top: 0;
    z-index: 10;
}

.bookedBar {
    position: relative;
    z-index: 3;
    border: 1px solid gold;
    margin: 0px;
    left: 0%;
    display: inline-block;
    /*transform: skewX(-20deg);*/
    background: #555;
    color: #fff;
    border-radius:20px;
    transition: 0.1s ease-in;
}
    .bookedBar:hover {
        transform: scale(1.1);
       cursor:pointer;
        background: linear-gradient(to top,#555,#262020);
        z-index:5;
    }
    .bookedBar.one {
        width: calc(65px*2);
    }
    .bookedBar.two {
        width: calc(65*3px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.three {
        width: calc(65*4px);
        background: #555;
        color: #fff;
    }
    .bookedBar.four {
        width: calc(65*5px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.five {
        width: calc(65*6px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.six {
        width: calc(65*7px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.seven {
        width: calc(65*8px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.eight {
        width: calc(65*9px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.nine {
        width: calc(65*10px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.ten {
        width: calc(65*11px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.eleven {
        width: calc(65*12px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar.twelve {
        width: calc(65*13px);
        background: #a97455;
        color: #fff;
    }
    .bookedBar p {
        display: flex;
        justify-content: center;
        font-size: 10px;
        color: gold;
        user-select: toggle;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
    }
    .popover {
        width: 600px;
        max-width: 95%;
    }
    table{
        border-collapse:collapse;width:100%;
    }
