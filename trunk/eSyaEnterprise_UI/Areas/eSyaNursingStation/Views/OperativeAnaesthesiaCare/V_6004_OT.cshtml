<div class="modal fade in" id="PopupAneasthesiaRecord" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-lg" role="document" style="box-shadow:none;">
        <div class="modal-content">
            <div class="modal-header popupHeader">
                <button type="button" class="close ml-auto" data-dismiss="modal"> <i class="fa fa-times"></i></button>
                <h4 class="modal-title">Aneasthesia Record</h4>
            </div>

            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label>Date</label>
                            <input class="form-control datepicker" type="text" maxlength="8" id="dpAneasthesiaRecordDate" />
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label>Time</label>
                            <input class="form-control timepicker" type="time" maxlength="8" id="tpAneasthesiaRecordTime" />
                        </div>
                    </div>
                </div>
                <div id="dvAneasthesiaRecordTemplate"></div>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-lg-12">
                        <button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent primary-button"
                                id="btnSaveVitalCharts" onclick="saveAneasthesiaRecord()">
